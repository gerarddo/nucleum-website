<% panelists.forEach( (panelist) => { %>
	<div class="row">
		<div class="thumbnail">

			<div class="container-fluid workshop-content panelist-content clearfix">
				<div class="col-2-3">
						<h2 class="pull-left"> <%= panelist.name %> </h2>
						<p class="pull-left text-justify"> <%= panelist.body %></p>
					</div>
					<div class="col-1-3">
						<img class="img-responsive panelist-img" src=" <%= panelist.image %>">
				</div>
			</div>
		</div>
	</div>
<%})%>

