<div class="row nucleum-orange-1tr" id="block2">
	<div class="col-1-1">
		<div class="container">
			<div class="row">
				<div class="col-1-2">
					<div class="panelists-img-container">
						<% include slideshow %>
					</div> <!-- .panelists-img-container -->
				</div> <!-- .col-1-2 -->
				<div class="col-1-2" id="change-width">
					<div class="panelists-header-container">
						<div class="container-fluid nucleum-blue-1tr">
							<h3 class="text-center">Conferencias</h3>
						</div>
					</div> <!-- .panelists-header-container -->
					<div class="panelists-content-container" >
						<div class="panelists-overlay" id="open-panelists-modal"></div>
						<div class="row text-center" >
							<div class="col-1-2 nucleum-black-1">
								<h3> <%= panelists[0].conference.name%></h3>
								<p> <%= panelists[0].conference.caption%></p>
							</div>
							<div class="col-1-2 nucleum-black-2">
								<h3> <%= panelists[1].conference.name%></h3>
								<p> <%= panelists[1].conference.caption%></p>
							</div>
							<div class="col-1-2 nucleum-black-2">
								<h3> <%= panelists[2].conference.name%></h3>
								<p> <%= panelists[2].conference.caption%></p>
							</div>
							<div class="col-1-2 nucleum-black-3">
								<h3> <%= panelists[3].conference.name%></h3>
								<p> <%= panelists[3].conference.caption%></p>
							</div>
							<div class="col-1-1 nucleum-black-1">
								<div class="last-panelist">
									<h3> <%= panelists[4].conference.name%></h3>
									<p> <%= panelists[4].conference.caption%></p>
								</div>
							</div>
						</div>
					</div> <!-- .panelists-content-container -->
				</div> <!-- .col-1-2 -->
				<div id="panelists-modal" class="modal">
				  <div class="modal-content-container container close-panelists-modal">
				    <% include ../modals/modal-Panelists %>
				  </div>
				</div> <!-- .modal -->
			</div>
		</div>
	</div>
</div>