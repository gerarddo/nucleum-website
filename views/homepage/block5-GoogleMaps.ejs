<div id="map" ></div>

		<script>
		  var map;
		  function initMap() {
		    map = new google.maps.Map(document.getElementById('map'), {
		      center: {lat: 25.6488054, lng: -100.28976979999999},
		      zoom: 16,
		      scrollwheel: false
		      // draggable: false
		    });
		    marker = new google.maps.Marker({
		      position: {lat: 25.6488054, lng: -100.28976979999999},
		      map: map
		    });
		  }
		</script>
		<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDTtE7rYSAr9ktp1HrXowX6K6hFUFuFXFg&callback=initMap"
		async defer></script>